<application>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="2" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="2000" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="USE_INSTANCEOF_ON_EQUALS_PARAMETER" value="true" />
    <option name="USE_ACCESSORS_IN_EQUALS_HASHCODE" value="true" />
    <EXCLUDED_PACKAGE NAME="java.util.Set" />
  </component>
  <component name="DaemonCodeAnalyzerSettings" profile="ide Default">
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="Groovy" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JAVA" value="true" />
        <entry key="JSON" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="kotlin" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="PREVIEW" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="false" />
    <option name="IS_CARET_BLINKING" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_DND_ENABLED" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="BIDI_TEXT_DIRECTION" value="LTR" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseOneLineMethods" value="false" />
    <option name="collapseSuppressWarnings" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="autoCloseTag" value="false" />
    <option name="automaticallyInsertClosingTag" value="false" />
    <option name="automaticallyInsertRequiredAttributes" value="false" />
    <option name="automaticallyInsertRequiredSubTags" value="false" />
    <option name="automaticallyStartAttribute" value="false" />
    <option name="insertQuotesForAttributeValue" value="false" />
    <option name="syncTagEditing" value="false" />
    <option name="tagTreeHighlightingEnabled" value="false" />
  </component>
</application>